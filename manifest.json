{
    "manifest_version": 2,
    
    "name": "repl.it-color",
    "version": "2.0.0",
    "description": "Customise the repl.it editor (https://repl.it) by changing the syntax highlighting to match your preferences. Created by @ArchieMaclean.",
    
    "options_page": "options.html",
    "permissions": ["storage"],
    "content_scripts": [{
        "js": ["storage.js", "script.js"],
        "matches": ["https://repl.it/*"]
     }],
     "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
     }
}
